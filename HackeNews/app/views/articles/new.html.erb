<h1> New Article <h1>


<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.label :numberComments %><br>
    <%= f.text_area :numberComments %>
  </p>
 
  <p>
    <%= f.label :votes %><br>
    <%= f.text_area :votes %>
  </p>
 


  <p>
    <%= f.submit %>
  </p>
<% end %>
